function largestOfFour(arr) {
  var max = arr[0][0];
  var maxarr = new Array(4);
  for (var i in arr) {
    for (var y in arr[i]) {
      if (arr[i][y] > max) {
        max = arr[i][y];
      }
      maxarr[i] = max;
    }
  }
  return maxarr;
}

largestOfFour([[4, 5, 1, 3], [13, 27, 18, 26], [32, 35, 37, 39], [1000, 1001, 857, 1]]);
