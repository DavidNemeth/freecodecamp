//We'll pass you an array of two numbers. Return the sum of those two numbers and all numbers between them.
function sumAll(arr) {
  var max = 0;
  var min = 0;
  var count = 0;
  if (arr[0]>arr[1])
    {
      max = arr[0];
      min = arr[1];
    }
  else {
    max = arr[1];
    min = arr[0];
  }
  for (var i = min;i < max+1 ;i++) {
    count += i;
  }
  return count;
}

sumAll([1, 4]);
